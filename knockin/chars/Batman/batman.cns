;Vs Style KO fall
[Statedef 5090]
type    = A
movetype= H
physics = N
facep2 = 1

[State 5090, 1]
type = VelAdd
trigger1 = Time > 0
y = GetHitVar(yaccel)

[State 5090, 2a]
type = VelSet
trigger1 = Time = 0 && Roundstate = 2 && roundstate != 3
x = -(abs(GetHitVar(xvel)) + (Const720p(20)))
y = -(abs(GetHitVar(yvel)) + (Const720p(50))*(abs(GetHitVar(yvel) < Const720p(56))))


[State 5090, 2b]
type = VelSet
trigger1 = Time = 0 && Roundstate = 3
x = -(abs(GetHitVar(xvel)) + (Const720p(20)))
y = -(abs(GetHitVar(yvel)) + (Const720p(50))*(abs(GetHitVar(yvel) < Const720p(56))))

[State 5090, 3] ;Anim for HIT_BACK/HIT_DIAG_UP
type = ChangeAnim
trigger1 = Vel Y < Const720p(-16)
value = Cond(SelfAnimExist(5035), 5035, 5030)

[State 5090, 4]
type = NotHitBy
trigger1 = Vel Y > Const720p(-8)
value = SCA
time = 700

[State 5090, 4]
type = ChangeState
trigger1 = Vel Y > 0
value = 5050

